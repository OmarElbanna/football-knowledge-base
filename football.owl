                        owl:someValuesFrom :Match
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :participates_in ;
                        owl:someValuesFrom :Championship
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_player ;
                        owl:minQualifiedCardinality "23"^^xsd:nonNegativeInteger ;
                        owl:onClass :Player
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_coach ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Coach
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_player ;
                        owl:maxQualifiedCardinality "30"^^xsd:nonNegativeInteger ;
                        owl:onClass :Player
                      ] .


